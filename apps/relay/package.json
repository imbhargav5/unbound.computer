{
  "name": "@unbound/relay",
  "version": "0.0.0",
  "private": true,
  "description": "Unbound Relay Server",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch --onSuccess 'node --env-file=.env dist/index.js'",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@unbound/protocol": "workspace:*",
    "@unbound/redis": "workspace:*",
    "@unbound/transport-reliability": "workspace:*",
    "pino": "^9.6.0",
    "ws": "^8.18.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@unbound/typescript-config": "workspace:*",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "pino-pretty": "^13.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
