{
  "name": "@unbound/daemon",
  "version": "0.0.18",
  "private": true,
  "scripts": {
    "test": "./scripts/test.sh",
    "build:go": "cd ../../packages/daemon-ably && go build -o daemon-ably ./cmd/daemon-ably && cd ../daemon-falco && go build -o falco ./cmd/falco && cd ../daemon-nagato && go build -o nagato ./cmd/nagato",
    "build": "pnpm build:go && cargo build --workspace --release",
    "build:dev": "pnpm build:go && dotenv -e .env.local -- cargo build --workspace",
    "build:prod": "pnpm build:go && dotenv -e .env.production.local -- cargo build --workspace --release",
    "dev": "dotenv -e .env.local -- cargo run --bin unbound-daemon",
    "start": "./target/debug/unbound-daemon",
    "start:release": "./target/release/unbound-daemon",
    "lint": "cargo clippy --workspace -- -D warnings",
    "typecheck": "cargo check --workspace"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2"
  }
}
