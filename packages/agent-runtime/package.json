{
  "name": "@unbound/agent-runtime",
  "version": "0.0.8",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@unbound/crypto": "workspace:*",
    "@unbound/protocol": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@unbound/typescript-config": "workspace:*",
    "@types/node": "^22.13.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
