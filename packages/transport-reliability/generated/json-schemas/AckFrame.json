{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "opcode": {
      "type": "string",
      "const": "ACK"
    },
    "eventId": {
      "type": "string",
      "format": "uuid",
      "pattern": "^([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-7[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12})$"
    }
  },
  "required": [
    "opcode",
    "eventId"
  ],
  "additionalProperties": false,
  "title": "AckFrame"
}
