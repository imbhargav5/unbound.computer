{
  "name": "@unbound/transport-reliability",
  "version": "0.0.6",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:json-schemas": "tsx scripts/generate-json-schemas.ts",
    "generate:swift": "claude \"Use the swift codegen generator skill and generate transport reliability swift file from the  src/schemas directory and generated json schemas in the generated/json-schemas directory\""
  },
  "dependencies": {
    "ulid": "^3.0.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.13.2",
    "@types/uuid": "^11.0.0",
    "@unbound/typescript-config": "workspace:*",
    "quicktype-core": "^23.2.6",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "uuid": "^13.0.0",
    "vitest": "^2.1.8"
  }
}
